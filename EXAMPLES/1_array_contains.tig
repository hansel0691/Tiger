let
	type arrayInt = array of int

	function Pertence(num:int,numeros:arrayInt,long:int):int =
		if long <= 0 then 0 
		else
			let
				var pertenece := 0
			in
				for i := 0 to long -1 do
					if num = numeros[i] then (pertenece := 1 ; break);
				pertenece
			end
	
	var l := 4
	var x := arrayInt[l] of 0
in
	x[0]:=2;
	x[1]:=3;
	x[2]:=-2;
	x[3]:=1;
	printi(Pertence(-2,x,l))
end
