/*Este programa debe imprimir los números del 0 al 9,
	10 veces cada uno y en líneas independientes*/

let
	type R = { x:int }
	var x:= 10
	type m = array of a
	type a = array of R
	type r = array of R
	var  a:= m[10] of a[10] of R { x = -1 }
in
	for j:= 0 to 9 do
		a[j]:= a[10] of R { x = j };
	for j:= 0 to 9 do
	(
		for k:= 0 to 9 do
		(
			printi(a[j][k].x);
			print(" ")
		);
		print("\n")
	)
end
