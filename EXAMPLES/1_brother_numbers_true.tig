/*Imprime si el numero de divisores de los numeros es igual*/

let
	function Hermanos(a:int,b:int):int=
		Divisores(a) = Divisores(b)

	function Divisores(n:int):int=
		let
			var sum := 1
		in
			for i := 2 to n -1 do 
				if n - i*(n/i) = 0 then 
					sum := sum + i;
		sum
	end

in
	printi(Hermanos(7,5))
end