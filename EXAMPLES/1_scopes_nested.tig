/*Este programa imprime 10, 1000, 990, 1, 2, ..., 10, -100, 10 
	en líneas independientes*/

let
	var a:= 10
in
	printi(a); 
	print("\n");
	let
		var a:= let 
					var a:= 1000 
				in 
					printi(a); 
					print("\n"); 
					a - 10 
				end
	in
		printi(a); 
		print("\n")
	end;
	for j:= 1 to 10 do ( 
		1; 
		2; 
		3; 
		printi(j); 
		print("\n") 
	);
	let
		var a := -100
	in
		printi(a); 
		print("\n")
	end;
	printi(a)
end
